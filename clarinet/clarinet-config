# set up environment
CLARINET_SETUP_DIR="$HOME/ai-agent-crew/clarinet"
CLARINET_BIN_DIR="$CLARINET_SETUP_DIR/bin"
CLARINET_DEPS_DIR="$CLARINET_SETUP_DIR/glibc-2.34"

# source this file to add clarinet to your PATH
clarinet_env() {
  LD_LIBRARY_PATH="$CLARINET_DEPS_DIR:/usr/lib/x86_64-linux-gnu:$LD_LIBRARY_PATH" "$CLARINET_BIN_DIR/clarinet" "$@"
}
alias clarinet=clarinet_env
export PATH="$CLARINET_BIN_DIR:$PATH"
