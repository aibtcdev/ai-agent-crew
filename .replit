entrypoint = "aibtc-v1/app.py"

run = "sh $HOME/ai-agent-crew/clarinet/setup-clarinet.sh && streamlit run aibtc-v1/app.py"

modules = ["python-3.11", "nodejs-18"]

hidden = [".pythonlibs", ".streamlit"]

[nix]
channel = "stable-24_05"

[env]
STREAMLIT_SERVER_HEADLESS = "true"

[deployment]
run = "sh $HOME/ai-agent-crew/clarinet/setup-clarinet.sh && streamlit --server.enableCORS=false --server.enableWebsocketCompression=false --server.runOnSave=false --client.toolbarMode=viewer run aibtc-v1/app.py"
ignorePorts = false
deploymentTarget = "gce"

[[ports]]
localPort = 8501
externalPort = 80
